    <script>
        setTimeout(function(){
            $("body").find(".window-app").each(function(){
                if(parseInt($(this).css('width')) < 840) {
                    $(this).addClass("ficha-mobile");
                } else {
                    $(this).removeClass("ficha-mobile");
                }
            });

            $("[fichaunica='{{strToCode actor.name}}'] [abrirmodalcontext]").contextmenu(function(e){
                e.preventDefault();
                $(this).parents(".ficha").find(".jsModalAberto").val($(this).attr("abrirmodalcontext")).trigger("change");
            });

            $("[fichaunica='{{strToCode actor.name}}'] [abrirModal]").click(function(e){
                e.preventDefault();
                $(this).parents(".ficha").find(".jsModalAberto").val($(this).attr("abrirModal")).trigger("change");
            });

            $("[fichaunica='{{strToCode actor.name}}'] [fecharModal]").click(function(e){
                $(this).parents(".ficha").find(".jsModalAberto").val("modaloff").trigger("change");
            });

            $("[fichaunica='{{strToCode actor.name}}'] [abrirAba]").click(function(e){
                e.preventDefault();
                $(this).parents(".ficha").find(".jsAbaAberta").val($(this).attr("abrirAba")).trigger("change");
            });


            //Numeros

            $("[fichaunica='{{strToCode actor.name}}'] .jsContaMaisAtributo, [fichaunica='{{strToCode actor.name}}'] .jsContaMenosAtributo").click(function(){
                var numeroAlvo = $(this).parent(".numero-box").find(".numero-input").val();
                var xpFakeDisponivel = $(this).parents(".ficha").find(".jsXpAtributosFakeInput").val();
                if(typeof(numeroAlvo) != "number") {
                    numeroAlvo = parseInt(numeroAlvo);
                }

                if(typeof(xpFakeDisponivel) != "number") {
                    xpFakeDisponivel = parseInt(xpFakeDisponivel);
                }


                if($(this).hasClass("jsContaMaisAtributo")) {
                    var proximoGasto = parseInt($(this).attr("valorproximo"));
                    console.log(proximoGasto);
                    console.log(xpFakeDisponivel);

                    if(proximoGasto > xpFakeDisponivel) {
                        alert("XP indisponível");
                    } else {
                        numeroAlvo += 1;
                        $(this).parent(".numero-box").find(".numero-input").val(numeroAlvo).trigger("change");
                    }
                    
                } else {
                    numeroAlvo -= 1;
                    $(this).parent(".numero-box").find(".numero-input").val(numeroAlvo).trigger("change");
                }

            });

            
            //Xp À Gastar

            var valorForca{{strToCode actor.name}} = parseInt($('[fichaunica="{{strToCode actor.name}}"] input[name="data.forca.graduacao"]').val());
            var valorReflexos{{strToCode actor.name}} = parseInt($('[fichaunica="{{strToCode actor.name}}"] input[name="data.reflexos.graduacao"]').val());
            var valorInteligencia{{strToCode actor.name}} = parseInt($('[fichaunica="{{strToCode actor.name}}"] input[name="data.inteligencia.graduacao"]').val());
            var valorEmpatia{{strToCode actor.name}} = parseInt($('[fichaunica="{{strToCode actor.name}}"] input[name="data.empatia.graduacao"]').val());

            var fNovo{{strToCode actor.name}} = -3;
            for (var Xa = 0; Xa <= valorForca{{strToCode actor.name}}; Xa++) {
                fNovo{{strToCode actor.name}} += Math.ceil(Xa/5)
            }

            var rNovo{{strToCode actor.name}} = -3;
            for (var Xb = 0; Xb <= valorReflexos{{strToCode actor.name}}; Xb++) {
                rNovo{{strToCode actor.name}} += Math.ceil(Xb/5)
            }

            var iNovo{{strToCode actor.name}} = -3;
            for (var Xc = 0; Xc <= valorInteligencia{{strToCode actor.name}}; Xc++) {
                iNovo{{strToCode actor.name}} += Math.ceil(Xc/5)
            }

            var eNovo{{strToCode actor.name}} = -3;
            for (var Xd = 0; Xd <= valorEmpatia{{strToCode actor.name}}; Xd++) {
                eNovo{{strToCode actor.name}} += Math.ceil(Xd/5)
            }

            var xpGasto{{strToCode actor.name}} = fNovo{{strToCode actor.name}}+rNovo{{strToCode actor.name}}+iNovo{{strToCode actor.name}}+eNovo{{strToCode actor.name}};
            var xpTotal{{strToCode actor.name}} = parseInt($('[fichaunica="{{strToCode actor.name}}"] [name="data.xp-atributos"]').val());

            $("[fichaunica='{{strToCode actor.name}}'] .jsXpAtributosFake").html((xpTotal{{strToCode actor.name}}-xpGasto{{strToCode actor.name}}).toString());
            $("[fichaunica='{{strToCode actor.name}}'] .jsXpAtributosFakeInput").val((xpTotal{{strToCode actor.name}}-xpGasto{{strToCode actor.name}}));
            


            $("[fichaunica='{{strToCode actor.name}}'] .ficha-perfil-atributos .ficha-xp, [fichaunica='{{strToCode actor.name}}'] .ficha-perfil-pericias .ficha-xp").contextmenu(function(){
                $(this).find(".minimodalAddXp").toggle();
            });

            $("[fichaunica='{{strToCode actor.name}}'] .minimodalAddXp a").click(function(){
                var acrescentarXp = parseInt($(this).parents(".minimodalAddXp").find("input").val());

                if(typeof(acrescentarXp)==="number") {
                    var acrescentarXpTotal = parseInt($(this).parents(".ficha").find('[name="data.xp-atributos"]').val());
                    $(this).parents(".ficha").find('[name="data.xp-atributos"]').val(acrescentarXpTotal+acrescentarXp).trigger("change");
                } else {
                    alert("Inválido");
                }
            });


            $("[fichaunica='{{strToCode actor.name}}'] .jsContaMaisAtributo, [fichaunica='{{strToCode actor.name}}'] .jsContaMenosAtributo").each(function(){
                var valorAtribuloGraduado = parseInt($(this).parents(".numero-box").find(".numero-input").val());

                if(valorAtribuloGraduado) {
                    if($(this).hasClass("jsContaMaisAtributo")) {
                        valorAtribuloGraduado += 1;
                        var valorAtribuloGraduadoArredondado = Math.ceil(valorAtribuloGraduado/5);
                        $(this).find(".numero-tooltip").html(" -"+valorAtribuloGraduadoArredondado+" xp atr");
                        $(this).attr("valorProximo",valorAtribuloGraduadoArredondado);
                        
                    } else {
                        valorAtribuloGraduado -= 1;
                        if(valorAtribuloGraduado > 0) {
                            $(this).show();
                            valorAtribuloGraduado += 1;
                            var valorAtribuloGraduadoArredondado = Math.ceil(valorAtribuloGraduado/5);
                            $(this).find(".numero-tooltip").html(" +"+valorAtribuloGraduadoArredondado+" xp atr");
                        } else {
                            valorAtribuloGraduado = 0;
                            $(this).hide();
                        }
                    }

                }


                $("[fichaunica='{{strToCode actor.name}}'] .jsEditavelChange").click(function(){
                    var editavelValor = $(this).attr("editavelValor");
                    if(editavelValor === "false") {
                        $(this).parents(".ficha").find('[name="data.editavel"]').val("true").trigger("change");
                    } else {
                        $(this).parents(".ficha").find('[name="data.editavel"]').val("false").trigger("change");
                    }
                });


                let editarFichaContagem{{strToCode actor.name}} = 0;
                $("[fichaunica='{{strToCode actor.name}}'] .ficha-perfil-corpo-img").contextmenu(function(){
                    editarFichaContagem{{strToCode actor.name}} += 1;

                    if(editarFichaContagem{{strToCode actor.name}} > 5) {
                        $("[fichaunica='{{strToCode actor.name}}'] .jsEditavelChangeLi").show();
                    }
                });
            });

            $("[fichaunica='{{strToCode actor.name}}'] .jsSelectFakeContext").contextmenu(function(){
                $(this).find(".ficha-selectfake-box").fadeToggle();
            });

			var pvMaxCalculado = $('[fichaunica="{{strToCode actor.name}}"] .jsPvMaximo').val();
			var pvMaxFicha = $('[fichaunica="{{strToCode actor.name}}"] [name="data.pv.max"]').val();
			if(pvMaxCalculado != pvMaxFicha) {
				$('[fichaunica="{{strToCode actor.name}}"] [name="data.pv.max"]').val(pvMaxCalculado);
			}

			var statusPv = $('[fichaunica="{{strToCode actor.name}}"] .jsPvStatus').val();
			var statusPvReal = $('[fichaunica="{{strToCode actor.name}}"] [name="data.pv.status"]').val();

			if(statusPv != statusPvReal) {
				$('[fichaunica="{{strToCode actor.name}}"] [name="data.pv.status"]').val(statusPv);
			}

			var fadigaMaxCalculado = $('[fichaunica="{{strToCode actor.name}}"] .jsFadigaMaxima').val();
			var fadigaMaxFicha = $('[fichaunica="{{strToCode actor.name}}"] [name="data.fadiga.max"]').val();
			if(fadigaMaxCalculado != fadigaMaxFicha) {
				$('[fichaunica="{{strToCode actor.name}}"] [name="data.fadiga.max"]').val(fadigaMaxCalculado);
			}

			var statusFadiga = $('[fichaunica="{{strToCode actor.name}}"] .jsFadigaStatus').val();
			var statusFadigaReal = $('[fichaunica="{{strToCode actor.name}}"] [name="data.fadiga.status"]').val();

			if(statusPv != statusPvReal) {
				$('[fichaunica="{{strToCode actor.name}}"] [name="data.fadiga.status"]').val(statusFadiga);
			}



			

        },300);
    </script>

    
        
